{
    "name": "fatch",
    "version": "0.0.0",
    "main": "dist/index.js",
    "esm": "dist/index.esm.js",
    "unpkg": "dist/fatch.min.js",
    "browser": "dist/fatch.min.js",
    "typings": "dist/index.d.ts",
    "repository": "https://github.com/fanhaoyuan/fatch.git",
    "author": "Fansy <418536538@qq.com>",
    "description": "A out-of-box http request library with fetch for modern browsers.",
    "keywords": [
        "http",
        "fetch",
        "stream",
        "http request",
        "request"
    ],
    "files": [
        "dist",
        "README.CN.md",
        "README.md",
        "CHANGELOG.md"
    ],
    "license": "MIT",
    "scripts": {
        "sandbox": "ts-node sandbox/server.ts",
        "dev": "ts-node build/dev.ts",
        "build": "ts-node build/build.ts && tsc"
    },
    "gitHooks": {
        "commit-msg": "cmv",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,ts,md,json,yml}": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "eslint"
        ]
    },
    "devDependencies": {
        "@fansy/commit-msg-validator": "^1.0.0",
        "@fansy/eslint-config": "^1.0.0",
        "@fansy/prettier-config": "^1.0.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.0.6",
        "@types/chokidar": "^2.1.3",
        "@types/lodash": "^4.14.176",
        "@types/node": "^16.11.0",
        "@types/rimraf": "^3.0.2",
        "chalk": "^4.1.2",
        "chokidar": "^3.5.2",
        "esbuild": "^0.13.11",
        "gzip-size": "^6.0.0",
        "lint-staged": "^11.2.3",
        "lodash": "^4.17.21",
        "rimraf": "^3.0.2",
        "rollup": "^2.58.3",
        "rollup-plugin-esbuild": "^4.6.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}
